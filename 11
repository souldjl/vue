             dataType: "html",
			 complete: function(data,textStatus)
			 {
				 $("#toploginarea").html(data.responseText);
			 }

         });

			$.ajax({
             type: "get",
             url: "logininfo.php?curtt="+top_now.getTime() +　+"&ttt="+top_now.getSeconds(),
