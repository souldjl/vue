.bookmenu span{ cursor:pointer;}
.bookmenu span.active{color:#4ac9bd; border-bottom:2px solid #4ac9bd;}
.bookmenu span:hover{color:#4ac9bd; border-bottom:2px solid #4ac9bd;}
.booklistwrap{width:1022px; height:148px; margin:22px 84px 0; position:relative;}

.booklist{width:1022px; height:148px; position:relative; overflow:hidden; display:none;}
.booklist.active{ display:block;}
.booklist ul{width:9999px; position:absolute; left:0; top:0;}
.booklist li{float:left; width:216px; height:160px; margin-right:52px;}
.booklist li img{ float:left; margin-left:10px;}
