.booklist li .txt p em{ color:#f13c40;}
.booklist li h5{ font-size:14px; line-height:24px; font-weight:normal; padding-top:4px; color:#333; text-align:center;}
.booklist li .txt .buy{ display:block; width:48px; height:20px; background:#f13c40; color:#fff; font-size:12px; line-height:20px; text-align:center; border-radius:3px;}

.booklistwrap span{ position:absolute; top:45px; width:26px; height:48px; background:url(../images/arr5.png) no-repeat; cursor:pointer; z-index:4;}
.booklistwrap .sliplefts{left:-64px; background-position:-52px 0;}
.booklistwrap .sliprights{right:-64px; background-position:-26px 0;}
.booklistwrap .sliplefts_1{left:-64px; background-position:-52px 0; display:none;}
.booklistwrap .sliprights_1{right:-64px; background-position:-26px 0; display:none;}



