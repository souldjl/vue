// JavaScript Document
$(function(){
	//鼠标经过qgfx时出现下拉
	$('.fxrk').hover(function() {	
		$('.fxList').css('display', 'block');
	}, function() {
		$('.fxList').css('display', 'none');
	});
	
	$(".fxrk li").click(function(event){
  
  		var $html = $(this).html();
  		$(".fxrk span em").html($html);
		
		}
	)
})